{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBucketIncomingPrefix",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::review-analyzer-briana-4nk3j3"],
      "Condition": { "StringLike": { "s3:prefix": ["incoming/*"] } }
    },
    {
      "Sid": "GetObjectsUnderIncoming",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": ["arn:aws:s3:::review-analyzer-briana-4nk3j3/incoming/*"]
    }
  ]
}
